graph dot {
  graph [
    fontsize = 10,
    rankdir = LR
    ]

  node [
    shape = circle,
    fontname = Helvetica
    ]
  A [label = "Seedling"]
  B [label = "Stage 1"]
  C [label = "Maturity"]

  edge[
    color = black,
    dir = forward
  ]
  A -- {B, A} [arrowhead = vee]
  B -- {C, B} [arrowhead = vee]
  C -- {A, C} [arrowhead = vee]
}